{
  "name": "ticketbot",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "prestart": "npm run lint && npm run format:check && npm run test",
    "start": "node src/index.js",
    "dev": "node src/index.js",
    "deploy": "node src/deploy-commands.js",
    "deploy:guild": "node src/deploy-commands.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "vitest": "^4.0.16"
  }
}
